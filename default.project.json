{
  "name": "RobloxVehicleSimulation",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/shared",
      "AskForDoor": {
        "$className": "RemoteEvent"
      },
      "SwitchLight": {
        "$className": "RemoteEvent"
      }
    },

    "ServerScriptService": {
      "$path": "src/server"
    },

    "StarterGui": {
      "$path": "src/ui"
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      },
      "StarterCharacterScripts": {
        "$path": "src/player"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Vehicles": {
        "$className": "Folder"
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}